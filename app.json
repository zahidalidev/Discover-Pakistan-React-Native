{
  "expo": {
    "name": "Discover Pakistan",
    "slug": "discover_pakistan_hd_tv",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.2",
    "sdkVersion": "38.0.0",
    "orientation": "portrait",
    "icon": "./App/Assets/icon_with_bg.png",
    "facebookScheme": "fb1620949568083766",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    },
    "splash": {
      "image": "./App/Assets/icon_with_bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.discoverpakistan.smartmultimedia",
      "buildNumber": "2.0.0",
      "supportsTablet": false,
      "usesAppleSignIn": true,
      "usesIcloudStorage": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The application requires access to your photos to allow you to upload pictures."
      }
    },
    "android": {
      "package": "com.app.discoverpakistan",
      "permissions": [
        "location"
      ],
      "versionCode": 6
    },
    "androidStatusBarColor": "#fff",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "Discover Pakistan Application is Updated Now - 18-May-2021"
  }
